@print-byte ( byte -- )
    DUP #04 SFT ,&char JSR
    &char ( char -- ) #0f AND DUP #09 GTH #27 MUL ADD #30 ADD #18 DEO
JMP2r

@print-short ( short^ -- )
    SWP print-byte print-byte
JMP2r

@print-long ( long^^ -- )
    SWP2 print-short print-short
JMP2r

@print-array ( arr* -- )
    STH2
    &next-byte
        LDA2kr STH2r #0000 
        EQU2 ,&end JCN

        LDAkr STHr print-byte

        INC2r
        ,&next-byte JMP
        
        &end
        POP2r
JMP2r
